@model IEnumerable<Dictionary<string, string>>;
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>

    <table class="table">
        <tr>
            <th>SecId</th>
            <th>ShortName</th>
            <th>CouponValue</th>
            <th>NextCoupon</th>
            <th>BoardName</th>
            <th>Status</th>
            <th>CouponPeriod</th>
            <th>Дата погашения</th>
        </tr>

        @foreach (var bond in Model)
        {
            <tr>
                <td>@bond["SECID"]</td>
                <td>@bond["SHORTNAME"]</td>
                <td>@bond["COUPONVALUE"]</td>
                <td>@bond["NEXTCOUPON"]</td>
                <td>@bond["BOARDNAME"]</td>
                <td>@bond["STATUS"]</td>
                <td>@bond["COUPONPERIOD"]</td>
                <td>@bond["MATDATE"]</td>
            </tr>
        }
    </table>
</div>
